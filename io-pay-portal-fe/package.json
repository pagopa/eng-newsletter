{
  "name": "io-pay-portal-frontend",
  "version": "0.1.0",
  "scripts": {
    "generate": "npm-run-all generate:*",
    "generate:api": "rimraf generated && mkdir -p generated  && gen-api-models --api-spec ../io-functions-pay-portal/openapi/index.yaml --out-dir ./generated --no-strict --request-types --response-decoders --client",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "parcel build index.html --public-url / ",
    "start": "parcel index.html",
    "start:proxy": "node api-proxy.js",
    "preversion": "auto-changelog  --config .auto-changelog.json --unreleased --commit-limit false --stdout --template preview.hbs",
    "version": "auto-changelog -p --config .auto-changelog.json --unreleased && git add CHANGELOG.md"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/express": "^4.17.9",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.5.2",
    "@types/react-dom": "^16.5.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "auto-changelog": "^2.2.1",
    "bootstrap-italia": "^1.4.1",
    "cors": "^2.8.5",
    "cssnano": "^4.1.10",
    "danger": "^10.5.3",
    "danger-plugin-digitalcitizenship": "^1.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "express": "^4.17.1",
    "italia-tslint-rules": "^1.1.3",
    "italia-utils": "^6.3.0",
    "jest": "^25.2.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.0",
    "promise-polyfill": "^8.2.0",
    "request": "^2.88.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^25.3.1",
    "tslint": "^5.1.0",
    "typescript": "^4.1.2"
  }
}
